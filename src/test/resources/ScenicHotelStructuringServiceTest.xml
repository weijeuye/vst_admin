<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xmlns:task="http://www.springframework.org/schema/task" xmlns:dubbo="http://code.alibabatech.com/schema/dubbo"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd
           http://www.springframework.org/schema/task
		   http://www.springframework.org/schema/task/spring-task-3.0.xsd
		   http://code.alibabatech.com/schema/dubbo
		   http://code.alibabatech.com/schema/dubbo/dubbo.xsd"
	default-lazy-init="true">

	<bean id="propertyConfigurer"
		class="com.lvmama.config.ZooKeeperPropertyPlaceholderConfigurer">
		<property name="systemPropertiesModeName" value="SYSTEM_PROPERTIES_MODE_OVERRIDE" />
		<property name="ignoreResourceNotFound" value="false" />
		<property name="locations">
			<list>
				<value>sweet-config.properties</value>
			</list>
		</property>
	</bean>

	<import resource="classpath*:applicationContext-vst-comm-dubbo-provider.xml" />

	<bean id="scenicHotelStructuringService"
		class="com.lvmama.vst.back.prod.scenicHotel.ScenicHotelStructuringServiceImpl" />

	<dubbo:reference id="prodPackageGroupClientService"
		interface="com.lvmama.vst.back.client.prod.service.ProdPackageGroupClientService" />

	<dubbo:reference id="hotelProdBranchQueryVstApiService"
		interface="com.lvmama.dest.api.vst.prod.service.IHotelProdBranchQueryVstApiService" />

	<dubbo:reference id="hotelProductQrVstApiService"
		interface="com.lvmama.dest.api.vst.prod.service.IHotelProductQrVstApiService" />
	<dubbo:reference id="scenicProdProductBranchService"
		interface="com.lvmama.scenic.api.back.prod.service.ScenicProdProductBranchService" />
	<dubbo:reference id="scenicProdProductService"
		interface="com.lvmama.scenic.api.back.prod.service.ScenicProdProductService" />
	<dubbo:reference id="suppGoodsHotelAdapterClientService"
		interface="com.lvmama.vst.back.client.goods.service.SuppGoodsHotelAdapterClientService" />
	<dubbo:reference id="scenicSuppGoodsService"
		interface="com.lvmama.scenic.api.prod.service.ScenicSuppGoodsService" />
	<dubbo:reference id="scenicTicketGoodsService"
		interface="com.lvmama.scenic.api.ticket.prod.service.ScenicTicketGoodsService" />
	<dubbo:reference id="prodLineRouteDescriptionService"
		interface="com.lvmama.vst.back.dujia.client.comm.route.service.ProdLineRouteDescriptionClientService" />
	<dubbo:reference id="prodLineRouteClientServiceRemote"
		interface="com.lvmama.vst.back.client.prod.service.ProdLineRouteClientService" />
	<dubbo:reference id="comLogClientServiceRemote"
		interface="com.lvmama.vst.back.client.pub.service.ComLogClientService" />
	<dubbo:reference id="prodProductDescriptionServiceRemote"
		interface="com.lvmama.vst.back.dujia.client.comm.prod.service.ProdProductDescriptionClientService" />
		
		
	<dubbo:reference id="prodProductServiceRemote" interface="com.lvmama.vst.back.prod.service.ProdProductService"/>

	<dubbo:reference id="multiDestInfoServiceRemote" interface="com.lvmama.phppid.dest.service.MultiDestInfoService"/>
	<dubbo:reference id="prodProductPropClientServiceRemote" 
		interface="com.lvmama.vst.back.client.prod.service.ProdProductPropClientService"/>
	
	<dubbo:reference id="hotelGoodsQueryVstApiServiceRemote" 
		interface="com.lvmama.dest.api.vst.goods.service.IHotelGoodsQueryVstApiService"/>
</beans>